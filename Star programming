import java.io.*;
import java.util.*;
class star
{
  public static void main(String args[])
  {
    int row;
    Scanner sc = new Scanner(System.in);
    System.out.println("enter no rows");
    row = sc.nextInt();
    for(int i=0; i<row; i++)
    {
      for(int j=0; j<=i; j++)
       {
          System.out.print("*");
       }
    System.out.println("");
    }
  }
}
